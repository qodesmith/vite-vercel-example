{
  "trailingSlash": false,
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ],
  "headers": [
    {
      "source": "/(.*).svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, must-revalidate, public"
        }
      ]
    }
  ]
}
